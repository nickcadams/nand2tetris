/**
 * Or gate: out = 1 if {a == 1 or b == 1}, 0 otherwise  
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
	And (a=a, b=a, out=c);
    And (a=b, b=b, out=d);
	Not (in=c, out=e);
	Not (in=d, out=f);
	And (a=e, b=f, out=g);
	Not (in=g, out=out);
}
